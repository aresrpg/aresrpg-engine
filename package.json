{
  "name": "aresrpg-engine",
  "private": true,
  "version": "0.1.1",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "publish": "npm run build && npm run lint",
    "build": "tsc -p src/lib/config/tsconfig.json",
    "clean": "shx rm -rf dist test/**/*.js*",
    "lint": "eslint src/lib -c src/lib/config/.eslintrc.json --ext .ts --fix",
    "test:http-server": "http-server ./test -p 8086 -c-1",
    "test:build": "webpack --config src/test/config/webpack.config.js"
  },
  "dependencies": {
    "three": "^0.162.0"
  },
  "devDependencies": {
    "@types/three": "^0.161.2",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.0",
    "http-server": "^14.1.1",
    "shx": "^0.3.4",
    "simplex-noise": "^4.0.1",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4"
  }
}
